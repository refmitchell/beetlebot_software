<launch>
  <node
      name="image_mask"
      pkg="bb_improc"
      type="image_mask_node"
      />
  <node
      name="gaussian_blur"
      pkg="bb_improc"
      type="gaussian_blur_node"
      />
  <node
      name="green_channel"
      pkg="bb_improc"
      type="green_channel_node"
      />
  <node
      name="bv_detector"
      pkg="bb_detection"
      type="intensity_cue_detector"
      args="-m bv -s green_frames -n bv_green_node -v"
      />
  <node
      name="cv_detector"
      pkg="bb_detection"
      type="intensity_cue_detector"
      args="-m cv -s green_frames -n cv_green_node -v"
      />
</launch>
